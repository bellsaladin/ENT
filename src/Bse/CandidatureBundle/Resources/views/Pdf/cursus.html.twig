﻿<h2><U><B><p align="center">Cursus Universitaire</p></B></U></h2><br>

<br><br>

					<table>
				<thead>
				
					<tr>	
						<th rowspan="2" >Année</th>	
						<th rowspan="2" style="width:45px;">Type Elp</th>
						<th style="width:220px" rowspan="2">Libellé Elp</th>
						<th style="width:90px;" colspan="2">Session 1</th>
						<th style="width:90px;" colspan="2">Session 2</th>
					</tr>
					
					<tr>			
						<th style="width:45px;">Note</th>
						<th style="width:45px;">Résultat</th>
						<th style="width:45px;">Note</th>
						<th style="width:45px;">Résultat</th>
					</tr>
				</thead>
				<tbody>
					{% set last_annee = 0 %}
					{% for cursus in elementsPedagogiques %}
					{% if cursus.codSession=="1" %}
						{% if cursus.codAnnee != last_annee %}
						<tr>
							<th colspan="7" bgcolor="#F9F9F9">
								{{ cursus.codAnnee }} / {{ cursus.codAnnee  + 1  }} 
							</th>
						</tr>
						{% endif %}
						<tr>
							    <td> </td>
								{% if cursus.libLse == "Module" %}
								<td style="width:45px;"><strong>Module</strong></td>
								<td style="padding-left:40px;" style="width:220px">{{ cursus.libElp }}</td>
								<td style="width:45px;">{{ cursus.note }}</td>
								<td style="width:45px;">{{ cursus.resultat }}</td>
								{% elseif cursus.libLse == "Elmt" %}
								<td style="width:45px;">Element</td>
								<td style="padding-left:60px;" style="width:220px">{{ cursus.libElp }}</td>
								<td style="width:45px;">{{ cursus.note }}</td>
								<td style="width:45px;">{{ cursus.resultat }}</td>
								{% elseif cursus.libLse == "Année" %}
								<td style="width:45px;"><strong>Année</strong></td>
								<td style="width:220px"><strong>{{ cursus.libElp }}</strong></td>
								<td style="width:45px;"><strong>{{ cursus.note }}</strong></td>
								<td style="width:45px;"><strong>{{ cursus.resultat }}</strong></td>
								{% else %}
								<td style="width:45px;"><strong>Semestre</strong></td>
								<td style="width:220px"><strong>{{ cursus.libElp }}</strong></td>
								<td style="width:45px;"><strong>{{ cursus.note }}</strong></td>
								<td style="width:45px;"><strong>{{ cursus.resultat }}</strong></td>
								{% endif %}
										
						
						{% for cursus1 in elementsPedagogiques %}
						{% if cursus1.codSession=="2" and cursus.codElp==cursus1.codElp and cursus.codAnnee==cursus1.codAnnee %}
							
							{% if cursus1.libLse == "Module" %}
								<td style="width:45px;">{{ cursus1.note }}</td>
								<td style="width:45px;">{{ cursus1.resultat }}</td>
								{% elseif cursus.libLse == "Elmt" %}
								<td style="width:45px;">{{ cursus1.note }}</td>
								<td style="width:45px;">{{ cursus1.resultat }}</td>
								{% elseif cursus.libLse == "Année" %}
								<td style="width:45px;"><strong>{{ cursus1.note }}</strong></td>
								<td style="width:45px;"><strong>{{ cursus1.resultat }}</strong></td>
								{% else %}
								<td style="width:45px;"><strong>{{ cursus1.note }}</strong></td>
								<td style="width:45px;"><strong>{{ cursus1.resultat }}</strong></td>

							{% endif %}


						{% endif %}
						{% endfor %}

						</tr>
						{% set last_annee = cursus.codAnnee %}
						{% endif %}
					{% endfor %}
		   		 </tbody>	
			</table>

		

