		{% else %}	
			<tr>
			<td style="width:100px" >{% if firstModuleOfSemestre %}Module {% endif  %}</td>
			<td style="padding-left:20px"> {{ module.libLse }}</td>
			</tr>
		{% endif %}

					
					{% set lastElementOfSemestre = false %}
					{% for element in module.children %}
						{% set firstElementOfSemestre = loop.first %}
						<tr>
							<td style="width:100px">{% if firstElementOfSemestre %}Element {% endif  %}</td>
							<td style="padding-left:40px">{{ element.libLse }}</td>
						</tr>
						
					{% endfor %}
    		{% endfor %}
